<script type="text/template" class="PageHeaderCFPItem">
    <div class="cfpItem">
        <a href="<@= link @>" class="unstyled articleLink cfpLinks">
            <@ if (showTopText) { @>
            <p class="topText bold"><@= topMessage @></p>
            <@ } @>
            <div class="imgContainer">
                <img src="<@= posterImg @>" />
            </div>
            <div class="movie_content_area">
                <span class="icon tiny certified"></span>
                <span class="tMeterScore"><@= score @>%</span>
            </div>
            <p class="title noSpacing"><@= movieName @></p>
        </a>
    </div>
</script>
<script type="text/template" class="PageHeaderListItem">
    <td class="left_col">
        <@ if (score > -1) { @>
            <@ if ("certified" === rating) { @>
                <span title="Certified&nbsp;Fresh" class="icon tiny certified"></span>
            <@ } else if ("fresh" === rating) { @>
                <span title="Fresh" class="icon tiny fresh"></span>
            <@ } else if ("rotten" === rating) { @>
                <span title="Rotten" class="icon tiny rotten"></span>
            <@ } @>
            <span class="tMeterScore"><@= score @>%</span>
        <@ } else { @>
            <span class="tMeterIcon tiny noRating">No Score Yet</span>
        <@ } @>
    </td>
    <td class="middle_col">
        <a id="tvPosterLink" href="<@= link @>"><@= showTitle @></a>
    </td>
</script>
