<script type="text/template" class="FeatureTemplate">
    <a href="<@= link @>" class="unstyled articleLink">
        <div class="bannerPic">
            <img src="<@= image @>" />
        </div>
        <div class="panel bannerCaption">
            <div class="panel-body">
                <p class="noSpacing title">
                    <@= header @>
                </p>
                <p class="byline noSpacing">
                    <@= body @>
                </p>
            </div>
        </div>
    </a>
</script>
<script type="text/template" class="HeadlineTemplate">
    <a href="<@= link @>" class="articleLink unstyled">
        <@= title @>
    </a>
    <br />
    <@ if (terms.publication && terms.publication.length > 0) { @>
        <span class="subtle">&ndash; <@= terms.publication[0].name @></span>
    <@ } @>
</script>
<script>
require([
    "globals",
    "jquery",
    "underscore",
    "backbone",
    "marionette"
], function(RT, $, _, Backbone, Marionette) {
    var Feature = Marionette.ItemView.extend({
        className: "newsItem col-xs-12",
        initialize: function(options) {
            this.template = options.childViewTemplate;
        }
    });
    var Features = Marionette.CollectionView.extend({
        childView: Feature,
        childViewOptions: {},
        initialize: function(options) {
            this.collection = options.collection;
            this.childViewOptions.childViewTemplate = options.FeatureTemplate;
        }
    });
    var Headline = Marionette.ItemView.extend({
        tagName: "li",
        initialize: function(options) {
            this.template = options.childViewTemplate;
        }
    });
    var Headlines = Marionette.CollectionView.extend({
        childView: Headline,
        childViewOptions: { },
        initialize: function(options) {
            this.collection = options.collection;
            this.childViewOptions.childViewTemplate = options.HeadlineTemplate;
        }
    });
    if (!RT.NewsThrottle.newsAndFeature) {
        require([
            RT.StaticHost + "/static/js/app/models/NewsModel.js"
        ], function(NewsModel) {
            var featuresItems = new NewsModel.Features(),
                headlinesItems = new NewsModel.NewsItems({
                    thirdPartyOnly: true,
                    limit: 7
                }),
                onFetchFeatures = $.Deferred(),
                onFetchHeadlines = $.Deferred();
            featuresItems.fetch({
                success: function() {
                    onFetchFeatures.resolve();
                }
            });
            headlinesItems.fetch({
                success: function() {
                    onFetchHeadlines.resolve();
                }
            });
            $.when(onFetchFeatures, onFetchHeadlines).then(function() {
                var features = new Features({
                        collection: featuresItems,
                        FeatureTemplate: _.template($("script.FeatureTemplate").html())
                    }),
                    headlines = new Headlines({
                        collection: headlinesItems,
                        HeadlineTemplate: _.template($("script.HeadlineTemplate").html())
                    });
                features.render();
                headlines.render();
                $("#FeaturedSection").html(features.$el);
                $("#latest-entertainment-headlines").html(headlines.$el);
            });
        });
    }
});
</script><div id="" class="panel panel-rt panel-box hidden" >
