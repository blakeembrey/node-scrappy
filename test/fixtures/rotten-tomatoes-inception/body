<div id="medrec_mobile_mob_bottom_ad" class="medrec_ad " style="width:300px"></div>
    <script>
                var mps=mps||{}; mps._queue=mps._queue||{}; mps._queue.mpsloaded=mps._queue.mpsloaded||[];
                mps._queue.gptloaded.push(function() {
                    if (mps.getResponsiveSet() =='0') { //MOBILE
                        mps.insertAd('#medrec_mobile_mob_bottom_ad','mboxadtwo');
                    }
                });
            </script>
