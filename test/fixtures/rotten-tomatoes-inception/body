    <div id="medrec_mobile_mob_bottom_ad" class="medrec_ad visible-xs center"></div>
    <script>
        require(["gpt-setup"], function(googletag){
            googletag.cmd.push(function() {
                var sizes = "true" ? [300,250] : [[300,250],[300,600]];
                var slot=googletag.defineSlot('/55153744/rottentomatoes/movie/movie_page', sizes, 'medrec_mobile_mob_bottom_ad')
                .addService(googletag.pubads())
                .setTargeting("pos", "mobile_mob_bottom");

                googletag.definedSlots['medrec_mobile_mob_bottom_ad']=slot;

                
            });
        });
    </script>



            
            
