        <script type="text/template" class="RecommendationItemTemplate">
    <a href="/m/<@= vanity @>/" class="unstyled articleLink">
        <div class="imgContainer">
            <img src="<@= posterImg @>" />
        </div>
        <div class="movie_content_area">
            <span class="icon tiny <@= scoreClass @>"></span>
            <span class="tMeterScore"><@= score @>%</span>
        </div>
        <p class="title noSpacing"><@= title @></p>
    </a>
</script>
<script type="text/template" class="NoShowtimesTemplate">
    <div>The movie doesn't seem to be playing near you.</div><br/>
</script>
<script type="text/template" class="ChangeShowtimesTemplate">
    <@ if (!noShowtimes) { @><a class="goBack">Go back</a><@ } @>
    <div>
        Enter your location to see where <em><@= movieTitle @></em> is playing near you
    </div>
    <div class="input-group col-xs-24 col-sm-12">
        <input class="typeahead form-control" type="text" value="" placeholder="Enter city & state or Zip Code"/>
        <span class="input-group-btn">
            <button id="location-button" type="button" class="btn btn-primary">
                Submit
            </button>
        </span>
    </div>
    <div class="results"></div>
</script>
<script type="text/template" class="NewsBox">
    <div id="contentNews" class="inlineBlock"></div>
</script>
<script type="text/template" class="NewsBoxItem">
    <a href="<@= link @>" class="unstyled articleLink">
        <div class="newsPhoto" style="background-image:url('<@= displayPhotoUrl @>')"></div>
        <div><@= title @></div>
    </a>
</script><script>
require([
    "globals",
    "jquery",
    "underscore",
    "js-cookie",
    "location-utils",
    "friend-ratings",
    "rtClamp"
], function(RT, $, _, Cookies, LocationUtils, mountFriendRatings) {
    var getView = function(ViewClass, viewData) {
        var view = new ViewClass(viewData);
        view.render();
        return view;
    };

    (function() {
        $("#heroImageContainer .playButton").addClass("in");

        $(".ratingButton").click(function() {
            $("html, body").animate({
                scrollTop: $("#rating_widget").offset().top - $(window).height()/2
            }, 1000);
        })
    }());

    // Modal Trailer
    (function() {
        require([RT.StaticHost + "/static/js/jspjs/modalTrailer.jsp.js"], function(ModalTrailer) {
            new ModalTrailer({ el: $("body"), vanity:'inception' });
        });
    }());

    // Movie Sidebar
    (function() {
        require([RT.StaticHost + "/static/js/app/views/MovieList.js"], function(MovieList) {
            new MovieList({
                el : $("#movies_sidebar"),
                scrollOnly: true,
                headerBottom: function() {
                    return $("#main_container").offset().top + 10;
                },
                footerTop: function()  {
                    return $(".sleaderboard_wrapper").offset().top;
                },
                options: false
            });
        });
    }());

    // Recommendation
    (function() {
        var onRecommendationFetch = function(recommendationItems) {
            if (recommendationItems.length > 0) {
                require([
                    RT.StaticHost + "/static/js/app/views/RecommendationView.js",
                    "slick"
                ], function(RecommendationView) {
                    var recommendationView = getView(RecommendationView.RecommendationPanel, {
                        collection: recommendationItems,
                        RecommendationTemplate: _.template($("script.RecommendationItemTemplate").html())
                    });
                    $("#recommendation .recommendationContainer").html(recommendationView.$el);
                    $("#recommendation .recommendationContainer .recPanel").on("init", function() {
                        $("#recommendation").css({
                            visibility: "visible",
                            height: ""
                        });
                    });
                    $("#recommendation .recommendationContainer .recPanel").slick({
                        lazyLoad: "ondemand",
                        slidesToShow:5,
                        slidesToScroll:5,
                        draggable: true,
                        infinite: true,

                        responsive:[{
                            breakpoint: 728,
                            settings: {
                                slidesToShow: 4,
                                slidesToScroll: 4
                            }
                        }, {
                            breakpoint: 610,
                            settings: {
                                slidesToShow:3,
                                slidesToScroll:3
                            }
                        },{
                            breakpoint: 375,
                            settings: {
                                slidesToShow:2,
                                slidesToScroll:2
                            }
                        }]
                    });
                });
            } else {
                $("#recommendation").remove();
            }
        };

        /** TODO: Recommendation API **/
        require([RT.StaticHost + "/static/js/app/models/RecommendationModel.js"], function(RecommendationModel) {
            var recommendationModel = new RecommendationModel.RecommendationModelItems({
                movieId: "770805418"
            });
            recommendationModel.fetch({
                success: onRecommendationFetch.bind(this, recommendationModel)
            });
        });
    }());

    //News panel
    (function() {
        var showNewsIfNotThrottled = function(condition, newsItem) {
            if (condition) {
                newsItem.fetch({
                    success: onNewsItemFetch.bind(this, newsItem)
                });
            }
        };
        if ("On" == RT.NewsThrottle.movie) {
            $("#newsSection").remove();
        } else {
            var onNewsItemFetch = function(newsItem) {
                if (newsItem.size() > 0) {
                    require([
                        RT.StaticHost + "/static/js/app/views/NewsView.js",
                    ], function(NewsView) {
                        var newsView = new NewsView.NewsPanel({
                            collection: newsItem,
                            NewsBoxTemplate: _.template($("script.NewsBox").html()),
                            NewsItemTemplate: _.template($("script.NewsBoxItem").html())
                        });
                        newsView.render();
                        $("#newsContainer").html(newsView.$el);
                        $("#newsSection").removeClass("hide");
                    });
                } else {
                    $("#newsSection").remove();
                }
            };
            require([RT.StaticHost + "/static/js/app/models/NewsModel.js"], function(NewsModel) {
                var newsItem = new NewsModel.NewsItems({
                    movieId: 770805418
                });
                if ("Off" == RT.NewsThrottle.movie) {
                    newsItem.fetch({
                        success: onNewsItemFetch.bind(this, newsItem)
                    });
                } else if ("Limited" == RT.NewsThrottle.movie) {
                    var mobWindow = "";
                    for (var i in dataLayer) {
                        var dataLayerVar = dataLayer[i];
                        if (dataLayerVar.event && "MOB Pageview" == dataLayerVar.event) {
                            mobWindow = dataLayerVar.mobWindow;
                            break;
                        }
                    }
                    //TODO: mobWindow needed
                    showNewsIfNotThrottled("Opening" == mobWindow || "InTheaters" == mobWindow || "TopBoxOffice" == mobWindow || "Upcoming" == mobWindow || "DvdNewRelease" == mobWindow, newsItem);
                } else {
                    showNewsIfNotThrottled(true || false, newsItem);
                }
            });
        }
    }());

    (function() {
        var showtimesView;
        var showtimeObject = {
            requestTheaterDataFromServer: function(locationDisplayName, locationLatitude, locationLongitude) {
                if (locationDisplayName) {
                    require([
                        RT.StaticHost + "/static/js/app/App.js",
                        RT.StaticHost + "/static/js/app/models/ShowtimesModel.js",
                        RT.StaticHost + "/static/js/app/views/MovieShowtimesView.js",
                        RT.StaticHost + "/static/js/app/views/ShowtimesView.js"
                    ], function(Showtimes) {
                        var showtimesQuery = {
                            date: 20160713,
                            deviceType: "RTO",
                            fullMovieInfo: true,
                            limit:1,
                            movie: 770805418,
                            showtimes: true,
                            vanityPlatform: "RTO",
                            latitude: locationLatitude,
                            longitude: locationLongitude
                        };
                        var showtimesData = new Showtimes.ShowtimesModel.TheaterData();
                        showtimesData.fetch({data:showtimesQuery}).then(function() {
                            if (showtimesData) {
                                require(["text!" + RT.StaticHost + "/static/templates/Movie.Showtimes.html"], function(MovieShowtimesTemplate) {
                                    if (showtimesData.attributes.hasOwnProperty("traits")) {
                                        showtimesView = new Showtimes.MovieShowtimesView.ShowtimesView({
                                            controller:this,
                                            isReleased:true,
                                            model:showtimesData,
                                            movieId: 770805418,
                                            MovieShowtimesTemplate:MovieShowtimesTemplate,
                                            showtimeDate:20160713,
                                            zipcode: locationDisplayName
                                        });
                                        showtimesView.render();
                                        $("#showtimes .showtimesContainer").html(showtimesView.$el);
                                    } else {
                                        require([
                                            RT.StaticHost + "/static/js/app/views/MovieShowtimesView.js"
                                        ], function(MovieShowtimesView) {
                                            $("#showtimes .showtimesContainer").html(
                                                    _.template($("script.NoShowtimesTemplate").html())() +
                                                    _.template($("script.ChangeShowtimesTemplate").html())({ movieTitle: "Inception", noShowtimes: true })
                                            );
                                            showtimesView = new MovieShowtimesView.LocationPicker({
                                                el: $("#showtimes .showtimesContainer"),
                                                controller: this,
                                                isReleased: true,
                                                showtimeDate: "20160713",
                                                zipcode:null
                                            });
                                        }.bind(this))
                                    }
                                }.bind(this));
                            }
                        }.bind(this));
                    }.bind(this));
                } else {
                    require([
                        RT.StaticHost + "/static/js/app/views/MovieShowtimesView.js"
                    ], function(MovieShowtimesView) {
                        $("#showtimes .showtimesContainer").html(
                            _.template($("script.NoShowtimesTemplate").html())() +
                            _.template($("script.ChangeShowtimesTemplate").html())({ movieTitle: "Inception", noShowtimes: true })
                        );
                        showtimesView = new MovieShowtimesView.LocationPicker({
                            el: $("#showtimes .showtimesContainer"),
                            controller: this,
                            isReleased: true,
                            showtimeDate: "20160713",
                            zipcode:null
                        });
                    }.bind(this))
                }
            },
            getShowtimes: function() {
                LocationUtils.getLocation()
                    .then(function(location) {
                        showtimeObject.requestTheaterDataFromServer(location.displayName, location.latitude, location.longitude);
                    });
            },
            onChangeLocation: function() {
                require([
                    RT.StaticHost + "/static/js/app/views/MovieShowtimesView.js"
                ], function(MovieShowtimesView) {
                    $("#showtimes .showtimesContainer").html(
                        _.template($("script.ChangeShowtimesTemplate").html())({
                            movieTitle: "Inception",
                            noShowtimes: false
                        })
                    );
                    $("#showtimes .showtimesContainer .goBack").click(function() {
                        LocationUtils.getLocation().then(function(location) {
                            showtimeObject.requestTheaterDataFromServer(location.displayName, location.latitude, location.longitude);
                        });
                    }.bind(this));
                    new MovieShowtimesView.LocationPicker({
                        controller: this,
                        el: $("#showtimes .showtimesContainer"),
                        isReleased: true,
                        showtimeDate: "20160713"
                    });
                }.bind(this));
            },
            onZipcodeUpdate: function(location) {
                LocationUtils.saveLocationPlaceId(location.placeId);
                showtimeObject.requestTheaterDataFromServer(location.displayName, location.latitude, location.longitude);
            }
        };
        showtimeObject.getShowtimes();
    }());


    // Load Friend reviews only if user is logged in
    RT.loginInit.always(function() {
        RT.loginModel.addOneSignInCallback(function() {
            mountFriendRatings(document.getElementById('friend-reviews-root'), 770805418);
        });
        RT.loginModel.addOneSignInFailCallback(function() {
            
            $("#friend-reviews-root").hide();
        });
        RT.loginModel.addOneSignInCallback(function(user) {
            console.info('arg in addSignInCallback is:', user);
            if (user && user.isCritic) {
                $("#critics-add-article").show();
            }
        });
        RT.loginModel.addOneSignInCallback(function(user) {
            var rolesMap = user.roles.reduce(function(prev, next) { prev[next] = true; return prev; }, {});
            $('#movie-admin-panel').html(_.template($('#adminPanelTemplate').html())({ roles: rolesMap }));
        });
    });

    // Load forum preview
    /** TODO: API Discussion **/
    // $.get("/m/" + options.movieId + "/jforum_preview").done(function(markup){
    //     $("#discussionForum .panel-body").html(markup);
    // });
});
</script>
</div>
        <script>console.info("body content:" + window.performance.now());</script>
        <div class="sleaderboard_wrapper hidden-xs">
		<div id="leaderboard_bottom_ad" style="width:728px;height:90px;margin-left:auto;margin-right:auto;">
		</div>
		<script>
			var mps=mps||{}; mps._queue=mps._queue||{}; mps._queue.mpsloaded=mps._queue.mpsloaded||[];
			mps._queue.gptloaded.push(function() {
				if (mps.getResponsiveSet() !='0') { //DESKTOP or TABLET
					mps.insertAd('#leaderboard_bottom_ad','bottombanner');
				}
			});
		</script>
	</div>
