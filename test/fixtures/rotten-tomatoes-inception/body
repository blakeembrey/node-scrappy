    <script>
        require([
            "jquery",
            "globals",
            "facebook",
            "https://staticv2.rottentomatoes.com/static/js/app/models/LoginModel.js",
            "bootstrap"
        ], function($, RT, FB, LoginModel) {
            RT.loginModel = new LoginModel();
            FB.Event.subscribe('auth.statusChange', function(response) {
                if (response.status == "connected") {
                    RT.loginModel.onFBLogin(response.authResponse);
                }
            });
            RT.loginInit.resolve();
        }.bind(this));
    </script>
