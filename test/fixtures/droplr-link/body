<script data-socket-status="" class="js-socket-connect">
function openSocketForDrop( event_callback ){

  $(".js-socket-connect").attr( "data-socket-status" , "initializing" );
    
  var pusher = new Pusher("5203af45f0a1996c8c30");
  var channel = pusher.subscribe('drop_q3z9');

  channel.bind( 'DROP_PREVIEW_GENERATED' , function( data ) {
    window.location.reload();
  });
  channel.bind( 'DROP_CREATED' , function( data ) {
    window.location.reload();
  });
  channel.bind( 'DROP_DATA_RECEIVED' , function( data ) {
    console.log("DROP_DATA_RECEIVED");
    if( event_callback !== undefined) event_callback( data );
  });
}
</script>

