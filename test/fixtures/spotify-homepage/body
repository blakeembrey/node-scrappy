      // Smooth scrolling for scroll links
      $('.btn-scroll').click(function() {
        if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'')
          || location.hostname == this.hostname) {
          var target = $(this.hash);
          target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
          if (target.length) {
            $('html,body').animate({
              scrollTop: target.offset().top
            }, 500);
            return false;
          }
        }
      });

      function updateCarouselHeight(){
        function thisHeight(){
          return $(this).outerHeight();
        }

        // Get tallest carousel slide height
        var tallestHeight = Math.max.apply(Math, $("#carousel-tour .item").map(thisHeight));

        // Apply tallest slide height to parent container, to avoid bouncing carousel controls
        $('#carousel-tour .carousel-inner').height(tallestHeight);

        // Apply top/bottom margin to shorter slides to vertically center
        // (Unable to do this with CSS due to carousel sliding CSS animations)
        $('.item').each(function() {
          var thisHeight = $(this).outerHeight();
          if (thisHeight < tallestHeight) {
            var slideMargin = Math.floor((tallestHeight - thisHeight) / 2);
            $(this).css({ 'margin-top': slideMargin + 'px', 'margin-bottom': slideMargin + 'px' });
          }
        });
      }

      updateCarouselHeight();

      // This is meant for the larger hero images
      $('.lazy').lazyload({
        effect: 'fadeIn',
        threshold : 500,
        effectspeed: 1000
      });

      // Updating lazy load images based on media query with javascript
      spweb.utils.updateLazyLoadImages();


      // If someone changes the window size, we need to update
      // the lazyloaded images and carousel height.
      $(window).resize(function () {
        spweb.utils.updateLazyLoadImages();
        updateCarouselHeight();
      });
    });
  </script>


