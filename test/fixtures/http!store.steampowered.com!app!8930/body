	var g_rgVACBannedSubs = [];
	// Wrap add to cart function in a confirm check
	if( g_rgVACBannedSubs.length > 0 )
	{
		var fnOldAdd = addToCart;
		addToCart = function( subid, dedupe )
		{
			if( g_rgVACBannedSubs.indexOf( subid ) != -1  )
				ShowConfirmDialog( "This account is VAC banned", "If you purchase this game, you will be unable to play on VAC secured servers with this account. For more information, see the <a %1$s>Help site<\/a>.".replace('%1$s', 'href="https://help.steampowered.com/en/wizard/HelpWithGame/?appid=8930"')).done( function(){ fnOldAdd( subid, dedupe );  } );
			else
				fnOldAdd( subid, dedupe );
		}
	}

	
