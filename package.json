{
  "name": "scrappy",
  "version": "0.3.0",
  "description": "Extract rich metadata from URLs",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "typings.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json --type-check",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm run clean && npm run tsc",
    "test:cov": "jest --coverage",
    "test:update": "jest -u",
    "test": "npm run lint && npm run build && npm run test:cov",
    "populate": "node scripts/fixtures.js && node scripts/scrape.js && node scripts/extract.js",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/node-scrappy.git"
  },
  "keywords": [
    "metadata",
    "scraper",
    "rich",
    "snippets",
    "info",
    "rdfa",
    "open graph",
    "oembed",
    "microdata",
    "json-ld"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blakeembrey/node-scrappy/issues"
  },
  "homepage": "https://github.com/blakeembrey/node-scrappy",
  "devDependencies": {
    "catbox-fs": "^2.0.0",
    "jest": "^22.1.3",
    "popsicle-cache": "^5.0.1",
    "rimraf": "^2.5.4",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "arrify": "^1.0.1",
    "concat-stream": "^1.5.2",
    "content-type": "^1.0.2",
    "exif-date": "^2.0.1",
    "exiftool2": "0.0.4",
    "getvalue": "^1.1.0",
    "htmlmetaparser": "^1.2.0",
    "htmlparser2": "^3.9.1",
    "jsonld": "^0.4.11",
    "popsicle": "^9.0.0"
  }
}
