{
  "name": "scrappy",
  "version": "0.2.3",
  "description": "Extract rich metadata from URLs",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "typings.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" -e \"src/typings/**\"",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test-spec": "blue-tape \"test/**/*.spec.js\" | tap-spec",
    "test-cov": "istanbul cover --print none node_modules/blue-tape/bin/blue-tape.js -- \"test/**/*.spec.js\" | tap-spec",
    "pretest": "npm run lint && npm run build",
    "test": "npm run test-cov",
    "populate": "node scripts/fixtures.js && node scripts/scrape.js && node scripts/extract.js",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/node-scrappy.git"
  },
  "keywords": [
    "metadata",
    "scraper",
    "rich",
    "snippets",
    "info",
    "rdfa",
    "open graph",
    "oembed",
    "microdata",
    "json-ld"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blakeembrey/node-scrappy/issues"
  },
  "homepage": "https://github.com/blakeembrey/node-scrappy",
  "devDependencies": {
    "blue-tape": "^0.2.0",
    "istanbul": "^0.4.3",
    "tap-spec": "^4.1.1",
    "thenify": "^3.2.0",
    "tslint": "^3.10.2",
    "tslint-config-standard": "^1.2.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "any-promise": "^1.3.0",
    "arrify": "^1.0.1",
    "content-type": "^1.0.2",
    "debug": "^2.2.0",
    "getvalue": "^1.1.0",
    "htmlparser2": "^3.9.1",
    "image-size-stream": "^1.1.0",
    "jpg-stream": "^1.1.1",
    "popsicle": "^8.0.0",
    "popsicle-status": "^2.0.0"
  }
}
