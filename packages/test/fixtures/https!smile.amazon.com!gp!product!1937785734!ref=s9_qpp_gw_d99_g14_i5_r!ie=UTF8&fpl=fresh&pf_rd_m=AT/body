         if (result === undefined || result === null) {
             return;
         }
         var urls = isReaderOpening ? result.onReaderOpen : result.onDetailPage;
         if (urls === undefined || urls === null) {
             return;
         }
         for (var i = 0; urls.length !== undefined && urls.length !== null && i < urls.length; i++) {
             jQuery.post(urls[i]);
         }
    };
