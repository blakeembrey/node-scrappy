	<script>
		jQuery(document).ready(function ($) {
			$( 'a.free-plugin-download' ).click( function() {
				$( '#free-plugin-download-modal' ).modal( {
					closeText:
						'<i class="fa fa-times-circle" aria-hidden="true"></i>' +
						'<span class="screen-reader-text">Close dialog</span>',
				} );

				position_modal();
			} );

			$(window).resize(position_modal);

			function position_modal() {
				var $modal_margin_top = ($(window).height() / 2) - ($('#free-plugin-download-modal').outerHeight() / 2) + 'px';
				var $modal_margin_left = ($(window).width() / 2) - ($('#free-plugin-download-modal').outerWidth() / 2) + 'px';

				$('#free-plugin-download-modal').css({
					'top': $modal_margin_top,
					'left': $modal_margin_left,
					'position': 'absolute'
				});
			}

			$.modal.enhance();
		});
	</script>
			<script>
			function register_open_modal() {
				jQuery( '[data-modal-product-id]' ).click( function( e ) {
					e.preventDefault();
					var product_id = jQuery( this ).data( 'modal-product-id' );

					jQuery( '#prices-modal-' + product_id ).modal( {
						closeText:
							'<i class="fa fa-times-circle" aria-hidden="true"></i>' +
							'<span class="screen-reader-text">Close dialog</span>',
					} );
				} );

				jQuery( '.modal label' ).click( function( e ) {
					e.preventDefault();
					jQuery( 'input#' + jQuery( this ).attr( 'for' ) ).prop( 'checked', true );
				} );

				$.modal.enhance();
			}

			jQuery( document ).ready( register_open_modal );
			jQuery( 'body' ).on( 'ab_ready', register_open_modal );
		</script>
		
